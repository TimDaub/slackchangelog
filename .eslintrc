{
    "rules": {
        "no-underscore-dangle": [
            0,
        ],
        "indent": [
            2,
            2
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "camelcase": [
            2,
            { properties: "never" }
        ]
    },
    "env": {
        "node": true,
        "amd": true,
        "mocha": true
    }
}
